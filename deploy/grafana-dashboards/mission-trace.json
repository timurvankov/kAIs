{
  "annotations": {
    "list": []
  },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "id": null,
  "links": [],
  "panels": [
    {
      "id": 1,
      "title": "Mission Duration",
      "type": "stat",
      "gridPos": { "h": 6, "w": 8, "x": 0, "y": 0 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "expr": "kais_mission_duration_seconds{mission=~\"$mission\"}",
          "legendFormat": "{{ mission }}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 60 },
              { "color": "red", "value": 300 }
            ]
          },
          "color": { "mode": "thresholds" },
          "unit": "s"
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": {
          "calcs": ["lastNotNull"],
          "fields": "",
          "values": false
        },
        "orientation": "auto",
        "textMode": "auto",
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "auto"
      }
    },
    {
      "id": 2,
      "title": "Cost Breakdown by Cell",
      "type": "piechart",
      "gridPos": { "h": 8, "w": 8, "x": 8, "y": 0 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "expr": "sum by (cell) (kais_cell_cost_total{mission=~\"$mission\"})",
          "legendFormat": "{{ cell }}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "unit": "currencyUSD"
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": {
          "calcs": ["lastNotNull"],
          "fields": "",
          "values": false
        },
        "pieType": "donut",
        "tooltip": { "mode": "single" },
        "legend": { "displayMode": "table", "placement": "right", "values": ["value", "percent"] }
      }
    },
    {
      "id": 3,
      "title": "Cell Activity Timeline",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 16, "x": 0, "y": 8 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "expr": "rate(kais_cell_llm_calls_total{mission=~\"$mission\"}[1m])",
          "legendFormat": "{{ cell }} - LLM calls",
          "refId": "A"
        },
        {
          "expr": "rate(kais_cell_messages_total{mission=~\"$mission\"}[1m])",
          "legendFormat": "{{ cell }} - Messages",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": {
            "drawStyle": "bars",
            "lineInterpolation": "linear",
            "fillOpacity": 80,
            "pointSize": 5,
            "showPoints": "never",
            "spanNulls": false,
            "lineWidth": 1,
            "stacking": { "mode": "normal", "group": "A" }
          },
          "color": { "mode": "palette-classic" },
          "unit": "ops"
        },
        "overrides": []
      },
      "options": {
        "tooltip": { "mode": "multi", "sort": "desc" },
        "legend": { "displayMode": "list", "placement": "bottom" }
      }
    },
    {
      "id": 4,
      "title": "Mission Traces",
      "description": "Links to Jaeger traces for the selected mission",
      "type": "table",
      "gridPos": { "h": 8, "w": 8, "x": 16, "y": 0 },
      "datasource": { "type": "jaeger", "uid": "jaeger" },
      "targets": [
        {
          "query": "$mission",
          "refId": "A",
          "queryType": "search",
          "service": "kais-operator",
          "operation": "",
          "limit": 20
        }
      ],
      "fieldConfig": {
        "defaults": {},
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "Duration" },
            "properties": [
              { "id": "unit", "value": "ms" }
            ]
          }
        ]
      },
      "options": {
        "showHeader": true,
        "footer": { "show": false },
        "sortBy": [{ "displayName": "Start Time", "desc": true }]
      }
    },
    {
      "id": 5,
      "title": "Message Flow",
      "description": "Recent messages between cells in the mission",
      "type": "table",
      "gridPos": { "h": 8, "w": 16, "x": 0, "y": 16 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "expr": "kais_cell_messages_total{mission=~\"$mission\"}",
          "legendFormat": "",
          "refId": "A",
          "format": "table",
          "instant": true
        }
      ],
      "fieldConfig": {
        "defaults": {},
        "overrides": []
      },
      "options": {
        "showHeader": true,
        "footer": { "show": false },
        "sortBy": [{ "displayName": "Value", "desc": true }]
      },
      "transformations": [
        {
          "id": "organize",
          "options": {
            "excludeByName": { "Time": true, "__name__": true },
            "renameByName": {
              "cell": "Cell",
              "direction": "Direction",
              "mission": "Mission",
              "Value": "Message Count"
            }
          }
        }
      ]
    }
  ],
  "refresh": "10s",
  "schemaVersion": 39,
  "tags": ["kais", "mission", "traces"],
  "templating": {
    "list": [
      {
        "current": {},
        "datasource": { "type": "prometheus", "uid": "prometheus" },
        "definition": "label_values(kais_mission_duration_seconds, mission)",
        "hide": 0,
        "includeAll": true,
        "label": "Mission",
        "multi": false,
        "name": "mission",
        "options": [],
        "query": {
          "query": "label_values(kais_mission_duration_seconds, mission)",
          "refId": "StandardVariableQuery"
        },
        "refresh": 2,
        "regex": "",
        "skipUrlSync": false,
        "sort": 1,
        "type": "query"
      }
    ]
  },
  "time": { "from": "now-1h", "to": "now" },
  "timepicker": {},
  "timezone": "browser",
  "title": "kAIs Mission Traces",
  "uid": "kais-mission-trace",
  "version": 1
}
